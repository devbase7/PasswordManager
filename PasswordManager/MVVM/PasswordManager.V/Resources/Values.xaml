<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:system="clr-namespace:System;assembly=mscorlib">
	<!--#region Doubles-->
	<system:Double
		x:Key="Double-ColumnWidthAndMinWidthRecordFields">
		300
	</system:Double>
	<!--#endregion-->


	<!--#region Strings-->
	<system:String
		x:Key="String-ToLogInThisFieldMustNotBeEmpty">
		To &quot;Log in&quot;, this field must not be empty!
	</system:String>


	<system:String
		x:Key="String-ToRegisterThisFieldMustNotBeEmpty">
		To &quot;Register&quot;, this field must not be empty!
	</system:String>


	<system:String
		x:Key="String-ToContinueThisFieldMustNotBeEmpty">
		To &quot;Continue&quot;, this field must not be empty!
	</system:String>


	<system:String
		x:Key="String-ToChangeThisFieldMustNotBeEmpty">
		To &quot;Change&quot;, this field must not be empty!
	</system:String>
	<!--#endregion-->
</ResourceDictionary>