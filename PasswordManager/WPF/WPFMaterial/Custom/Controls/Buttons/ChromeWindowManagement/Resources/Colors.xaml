<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<!--#region Colors-->
	<Color
		x:Key="Color-Background-Normal">
		Silver
	</Color>


	<Color
		x:Key="Color-Icon-Normal">
		#999999
	</Color>


	<Color
		x:Key="Color-Icon-MouseOver">
		White
	</Color>


	<Color
		x:Key="Color-Icon-Pressed">
		White
	</Color>
	<!--#endregion-->


	<!--#region SolidColorBrushes-->
	<SolidColorBrush
		x:Key="SolidColorBrush-Border"
		Color="Transparent"/>


	<SolidColorBrush
		x:Key="SolidColorBrush-BorderMouseOver"
		Color="#5593FF"/>


	<SolidColorBrush
		x:Key="SolidColorBrush-BorderPressed"
		Color="#3C77DD"/>


	<SolidColorBrush
		x:Key="SolidColorBrush-Icon"
		Color="{StaticResource Color-Icon-Normal}"/>
	<!--#endregion-->
</ResourceDictionary>